# Navega a la carpeta de tu proyecto (donde está el código de Astro)
cd ruta/de/tu/proyecto

# Inicializa Git (si no lo hiciste antes)
git init

# Conecta tu proyecto con GitHub
git remote add origin https://github.com/tu-usuario/mi-sitio-astro.git

# Sube todo el código
git add .
git commit -m "Primer commit"
git branch -M main
git push -u origin main
